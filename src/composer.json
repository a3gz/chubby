{
    "name": "a3gz/chubby",
    "description": "Chubby dependencies",
    "require": {
        "slim/slim": "3.5",
        "monolog/monolog": "^1.23",
        "a3gz/chubby-view": "^2"
    },
    "autoload": {
        "psr-4": {
            "Controllers\\": "./app/controllers",
            "Templates\\": "./app/views/templates"
        }
    },
    "require-dev": {
        "php": "7.3",
        "phpunit/phpunit": "^9"
    },
    "scripts": {
        "test": "phpunit test/php",
        "test-verbose": "phpunit --verbose test/php",
        "debug-test": "phpunit --debug test/php"
    }
}