{
    "name": "a3gz/chubby",
    "description": "Chubby dependencies",
    "require": {
        "slim/slim": "4.3.0",
        "monolog/monolog": "^1.23",
        "a3gz/chubby-view": "^2",
        "php-di/php-di": "6.0.11",
        "slim/psr7": "^1.3",
        "bainternet/php-hooks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "./app",
            "Plugins\\": "./app/plugins",
            "Templates\\": "./app/templates",
            "Core\\": "./core",
            "Fat\\": "./fat"
        }
    },
    "require-dev": {
        "php": "^7.4",
        "phpunit/phpunit": "^9"
    },
    "scripts": {
        "test": "phpunit test/php",
        "test-verbose": "phpunit --verbose test/php",
        "debug-test": "phpunit --debug test/php"
    }
}
